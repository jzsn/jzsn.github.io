			<span class="p">{</span>
				<span class="n">fiv</span><span class="p">.</span><span class="n">emplace_back</span><span class="p">(</span><span class="n">p</span><span class="p">.</span><span class="n">second</span><span class="p">,</span> <span class="n">pre</span><span class="p">);</span>
