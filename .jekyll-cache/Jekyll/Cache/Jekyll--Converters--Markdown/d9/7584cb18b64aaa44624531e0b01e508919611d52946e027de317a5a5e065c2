		<span class="p">}</span>
	<span class="p">}</span>
	<span class="k">while</span> <span class="p">(</span><span class="n">sev</span><span class="p">.</span><span class="n">size</span><span class="p">()</span> <span class="o">&gt;</span> <span class="n">fiv</span><span class="p">.</span><span class="n">size</span><span class="p">())</span>
	<span class="p">{</span>
