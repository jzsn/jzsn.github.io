		<span class="n">sev</span><span class="p">.</span><span class="n">pop_back</span><span class="p">();</span>
	<span class="p">}</span>
	<span class="n">printf</span><span class="p">(</span><span class="s">"%d</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">sev</span><span class="p">.</span><span class="n">size</span><span class="p">());</span>
	<span class="k">while</span> <span class="p">(</span><span class="o">!</span><span class="n">sev</span><span class="p">.</span><span class="n">empty</span><span class="p">())</span>
	<span class="p">{</span>
		<span class="n">printf</span><span class="p">(</span><span class="s">"%s %s</span><span class="se">\n</span><span class="s">%s %s</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">s</span> <span class="o">+</span> <span class="n">st</span><span class="p">[</span><span class="n">fiv</span><span class="p">.</span><span class="n">back</span><span class="p">().</span><span class="n">first</span><span class="p">],</span> <span class="n">s</span> <span class="o">+</span> <span class="n">st</span><span class="p">[</span><span class="n">sev</span><span class="p">.</span><span class="n">back</span><span class="p">().</span><span class="n">first</span><span class="p">],</span> <span class="n">s</span> <span class="o">+</span> <span class="n">st</span><span class="p">[</span><span class="n">fiv</span><span class="p">.</span><span class="n">back</span><span class="p">().</span><span class="n">second</span><span class="p">],</span> <span class="n">s</span> <span class="o">+</span> <span class="n">st</span><span class="p">[</span><span class="n">sev</span><span class="p">.</span><span class="n">back</span><span class="p">().</span><span class="n">second</span><span class="p">]);</span>
		<span class="n">sev</span><span class="p">.</span><span class="n">pop_back</span><span class="p">();</span>
		<span class="n">fiv</span><span class="p">.</span><span class="n">pop_back</span><span class="p">();</span>
	<span class="p">}</span>
<span class="p">}</span>
</code></pre></div></div>

