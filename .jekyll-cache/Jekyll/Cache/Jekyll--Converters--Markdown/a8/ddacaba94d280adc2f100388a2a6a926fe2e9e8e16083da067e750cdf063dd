		<span class="c1">//printf("ret=%lld\n",ret);</span>
		<span class="n">u</span> <span class="o">+=</span> <span class="n">x</span><span class="p">;</span>
		<span class="n">t</span> <span class="o">=</span> <span class="p">(</span><span class="n">t</span> <span class="o">*</span> <span class="mi">2</span><span class="p">)</span> <span class="o">%</span> <span class="n">mod</span><span class="p">;</span>
