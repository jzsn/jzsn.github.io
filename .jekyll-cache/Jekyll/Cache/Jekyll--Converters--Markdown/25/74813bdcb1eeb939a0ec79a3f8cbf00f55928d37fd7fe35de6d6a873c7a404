<h3 id="Ê∫ê‰ª£Á†Åhelloworldcu">Ê∫ê‰ª£Á†Å<code class="highlighter-rouge">HelloWorld.cu</code></h3>

<pre><code class="language-clike">#include &lt;stdio.h&gt;
__global__ void kernel()
{
	printf("Hello world from block(%d,%d,%d) thread(%d,%d,%d).\n",
		   blockIdx.x, blockIdx.y, blockIdx.z,
		   threadIdx.x, threadIdx.y, threadIdx.z);
}
int main()
{
	dim3 grid(2, 4), block(8, 16);
	kernel&lt;&lt;&lt;grid, block&gt;&gt;&gt;();
	cudaDeviceSynchronize();
}
</code></pre>

<h3 id="Ë∞ÉÂ∫¶ËÑöÊú¨helloworldpbs">Ë∞ÉÂ∫¶ËÑöÊú¨<code class="highlighter-rouge">HelloWorld.pbs</code></h3>

<div class="language-bash highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="c">#PBS -N HelloWorld</span>
<span class="c">#PBS -l nodes=1:ppn=32:gpus=1</span>
<span class="c">#PBS -j oe</span>
<span class="c">#PBS -q gpu</span>

<span class="nb">source</span> /public/software/profile.d/cuda10.0.sh
<span class="nb">cd</span> <span class="nv">$PBS_O_WORKDIR</span>
nvcc HelloWorld.cu <span class="nt">-o</span> HelloWorld
./HelloWorld
</code></pre></div></div>

