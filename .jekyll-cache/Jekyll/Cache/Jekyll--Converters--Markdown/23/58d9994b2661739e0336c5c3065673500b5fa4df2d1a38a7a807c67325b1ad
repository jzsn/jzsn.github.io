	<span class="p">}</span>
	<span class="c1">//int ch(int u, int i = 0) { return e[v[u].o[i]].second; } //uçš„ç¬¬iä¸ªå­©å­èŠ‚ç‚¹</span>
	<span class="c1">//int fa(int u, int i = 0) { return e[v[u].i[i]].first; }  //uçš„ç¬¬iä¸ªçˆ¶èŠ‚ç‚¹</span>
<span class="p">};</span>
<span class="k">struct</span> <span class="nc">ISAP</span> <span class="o">:</span> <span class="n">Graph</span>
