<span class="p">{</span>
	<span class="k">struct</span> <span class="nc">Vertex</span>
	<span class="p">{</span>
		<span class="n">vector</span><span class="o">&lt;</span><span class="kt">int</span><span class="o">&gt;</span> <span class="n">o</span><span class="p">;</span> <span class="c1">//, i;		//ç›¸å…³å‡ºè¾¹å’Œå…¥è¾¹ç¼–å·</span>
