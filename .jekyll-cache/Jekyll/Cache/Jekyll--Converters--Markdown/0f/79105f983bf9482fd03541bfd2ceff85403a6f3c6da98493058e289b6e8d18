	<span class="n">RegisterFile</span> <span class="n">rf</span><span class="p">(</span><span class="n">CLK</span><span class="p">,</span> <span class="n">RegWre</span><span class="p">,</span> <span class="n">rs</span><span class="p">,</span> <span class="n">rt</span><span class="p">,</span> <span class="n">WriteReg</span><span class="p">,</span> <span class="n">WriteData</span><span class="p">,</span> <span class="n">ReadData1</span><span class="p">,</span> <span class="n">ReadData2</span><span class="p">);</span>
	<span class="n">ALU</span> <span class="n">alu</span><span class="p">(</span><span class="n">ALUOp</span><span class="p">,</span> <span class="n">A</span><span class="p">,</span> <span class="n">B</span><span class="p">,</span> <span class="n">zero</span><span class="p">,</span> <span class="n">result</span><span class="p">,</span><span class="n">sign</span><span class="p">);</span>
	<span class="n">SignZeroExtend</span> <span class="n">sze</span><span class="p">(</span><span class="n">ExtSel</span><span class="p">,</span> <span class="n">immediate</span><span class="p">,</span> <span class="n">extendImmediate</span><span class="p">);</span>
	<span class="n">DataMemory</span> <span class="n">dm</span><span class="p">(</span><span class="n">DataMemRW</span><span class="p">,</span> <span class="n">RegResult</span><span class="p">,</span> <span class="n">RegReadData2</span><span class="p">,</span> <span class="n">DataOut</span><span class="p">);</span>
