		<span class="n">IF</span><span class="o">=</span>	<span class="mb">3'b000</span><span class="p">,</span>	<span class="c1">//3位D触发器，代表8个状态</span>
		<span class="n">ID</span><span class="o">=</span>	<span class="mb">3'b001</span><span class="p">,</span>
		<span class="n">EXELS</span><span class="o">=</span>	<span class="mb">3'b010</span><span class="p">,</span>
		<span class="n">MEM</span><span class="o">=</span>	<span class="mb">3'b011</span><span class="p">,</span>
