	<span class="kt">reg</span><span class="p">[</span><span class="mi">2</span><span class="o">:</span><span class="mi">0</span><span class="p">]</span> <span class="n">D_Tri</span><span class="p">;</span>
	<span class="k">initial</span> <span class="k">begin</span>
		<span class="n">PCWre</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
		<span class="n">ALUSrcB</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
		<span class="n">ALUM2Reg</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
		<span class="n">RegWre</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
		<span class="n">WrRegData</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
		<span class="c1">//no change</span>
		<span class="n">InsMemRW</span><span class="o">=</span><span class="mi">0</span><span class="p">;</span>
