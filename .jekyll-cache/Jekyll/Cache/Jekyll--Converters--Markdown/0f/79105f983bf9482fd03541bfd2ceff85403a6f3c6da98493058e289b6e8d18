      <th>10-6</th>
      <th>5-0</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>op</td>
      <td>rs</td>
      <td>rt</td>
      <td>rd</td>
      <td>sa</td>
      <td>func</td>
    </tr>
    <tr>
      <td>6位</td>
      <td>5位</td>
      <td>5位</td>
      <td>5位</td>
      <td>5位</td>
      <td>6位</td>
    </tr>
  </tbody>
</table>

